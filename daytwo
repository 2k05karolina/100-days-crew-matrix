//odd or even using function
import java.util.*;
class Find {
    void evenorodd(int num){
        if(num%2==0){
            System.out.println("even");
     }
     else{
        System.out.println("odd");
    }
        
    }

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        Find obj1=new Find();
        obj1.evenorodd(num);
        
    }
}
