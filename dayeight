import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter Alice height:");
        int x=sc.nextInt();
        System.out.println("Enter Bob height:");
        int y=sc.nextInt();
       {
           if(x<y){
              System.out.println("B"); 
           }
           else{
               System.out.println("A"); 
           }
           
       }    
     
    }
}
